p2:p2.cpp
	g++ -DONLINE_JUDGE -O2 -w -fmax-errors=3 -std=c++17 p2.cpp -lm -o p2
p3:p3.cpp
	g++ -DONLINE_JUDGE -O2 -w -fmax-errors=3 -std=c++17 p3.cpp -lm -o p3
p4:p4.cpp
	g++ -DONLINE_JUDGE -O2 -w -fmax-errors=3 -std=c++17 p4.cpp -lm -o p4
bonus:bonus.cpp
	g++ -DONLINE_JUDGE -O2 -w -fmax-errors=3 -std=c++17 bonus.cpp -lm -o bonus

